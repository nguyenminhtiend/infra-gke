{
  "name": "infra-gke",
  "version": "1.0.0",
  "description": "GKE Infrastructure and Applications Monorepo",
  "private": true,
  "scripts": {
    "dev": "pnpm run --parallel --recursive start:dev",
    "build": "pnpm run --recursive build",
    "test": "pnpm run --recursive test",
    "lint": "pnpm run --recursive lint",
    "format": "pnpm run --recursive format",
    "clean": "pnpm run --recursive clean && rm -rf node_modules",
    "install:all": "pnpm install",
    "docker:build": "pnpm run --recursive docker:build",
    "docker:push": "pnpm run --recursive docker:push"
  },
  "keywords": [
    "kubernetes",
    "gke",
    "nestjs",
    "microservices",
    "monorepo"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "devDependencies": {
    "@types/node": "^22.5.4",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  }
}
